# Checklist Overlay Configuration

# WebSocket Service URL
VITE_WS_URL=ws://localhost:8080

# API Base URL
VITE_API_URL=http://localhost:8080

# Default Theme
VITE_DEFAULT_THEME=dark

# Default Position
VITE_DEFAULT_POSITION=top-left

# Default Size Scale
VITE_DEFAULT_SCALE=1.0

# Animation Settings
VITE_ANIMATION_DURATION=300
VITE_ENABLE_ANIMATIONS=true

# Debug Mode
VITE_DEBUG=false

# Development
NODE_ENV=development